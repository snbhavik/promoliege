<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.mikodigital.promosliege" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>commikodigitalpromosliege</name>
  <description>
 Toutes vos promotions en une application 
</description>
  <author email="Miko Digital LLC" href="">
 Miko Digital LLC 
</author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <preference name="splashscreen" value="splashscreen"/>
