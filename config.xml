  <description>
 Toutes vos promotions en une application 
</description>
  <author email="Miko Digital LLC" href="">
 Miko Digital LLC 
</author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
