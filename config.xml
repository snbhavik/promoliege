<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.mikodigital.promosliege" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>commikodigitalpromosliege</name>
  <description>
 Toutes vos promotions en une application 
</description>
  <author email="Miko Digital LLC" href="">
 Miko Digital LLC 
</author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <preference name='phonegap-version' value='cli-7.0.1' />
  <platform name="android">
    <allow-intent href="market:*"/>
    <preference name="splashscreen" value="splashscreen"/>
    <preference name="splashScreenDelay" value="10000"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <plugin name="cordova-plugin-file" spec="~1" source="npm"/>
  <plugin name="cordova-plugin-file-transfer" spec="~1" source="npm"/>
  <plugin name="cordova-plugin-inappbrowser" spec="~1" source="npm"/>
  <plugin name="cordova-plugin-network-information" spec="~1" source="npm"/>
  <plugin name="cordova-plugin-splashscreen" spec="~1" source="npm"/>
  <plugin name="cordova-plugin-statusbar" spec="~1" source="npm"/>
  <plugin name="cordova-plugin-vibration" spec="~1" source="npm"/>
  <plugin name="cordova-plugin-whitelist" spec="~1" source="npm"/>
  <plugin name="com.phonegap.plugins.PushPlugin" spec="3.2.0" source="npm"/>
  <plugin name="nl.x-services.plugins.socialsharing" spec="5.0.11" source="npm"/>
  <plugin name="com.telerik.cordovaPlatformSpecificFiles" spec="~1" source="npm"/>
</widget>